  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:local": "concurrently \"yarn run start:dev service-core\" \"yarn run start:dev service-integration\" \"yarn run start:dev service-user\" \"yarn run start:dev service-notification\" \"yarn run start:dev service-file\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/tech/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/tech/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "yarn run typeorm -- migration:create",
    "typeorm:migration:generate": "yarn run typeorm -- migration:generate -d apps/service-core/src/common/data-sources/ds.ts",
    "typeorm:migration:up": "yarn run typeorm -- migration:run -d apps/service-core/src/common/data-sources/ds.ts",
    "typeorm:migration:down": "yarn run typeorm -- migration:revert -d apps/service-core/src/common/data-sources/ds.ts"
  },
